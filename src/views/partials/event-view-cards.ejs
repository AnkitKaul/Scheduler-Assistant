<div class="row">
  <% for (let event of events) { %>
    <div class="col-lg-6 my-4">
      <div class="card" style="width:30rem; height:5rem">
        <h5 class="card-header">
          <a href="/event/id/<%= event.eventId %>" class="text-primary">
            <%= event.eventName %>
          </a>
        </h5>
        <%- include event-preview %>
      </div>
    </div>
    <% } %>
</div>